                                      {% if is_external %}
                                      <div>
                                        <p class="text p" style="display: block; float: left; margin: 14px 0; margin-left: 20px; margin-top: 6px;font-family: MicrosoftYaHei;font-size: 12px;color: #EA3636;letter-spacing: 0;line-height: 20px;">
                                          {{sensitive_message}}
                                        </p>

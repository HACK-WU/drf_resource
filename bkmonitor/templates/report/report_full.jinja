									{% for item in contents %}
									    <p class="content-title text p" style="display: block; margin: 14px 0; font-family: Helvetica,Arial,sans-serif; line-height: 20px; padding-left: 4px; border-left: 4px solid #699DF4; color: #2E3959; font-size: 14px; margin-left: 18px; margin-top: 20px;">{{ item.title }}</p>
                                        <p class="content-tip text p" style="display: block; font-family: Helvetica,Arial,sans-serif; line-height: 20px; color: #979BA5; font-size: 12px; margin: 10px 0 16px 18px;">{{ item.content }}</p>
                                        {% for graphs in item.graphs %}
                                      <img src="cid:__INLINE__{{ graphs[0]['cid_tag'] }}" class="content-img img__block" border="0" alt="" style="display: block; width: 1600px; max-width: 1600px; margin: 0 auto;" width="1600" />
									  {% endfor %}
                                    {% endfor %}
